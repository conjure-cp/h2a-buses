    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-plugin-vue": "^9.26.0",
    "npm-run-all2": "^6.2.0",
    "postcss": "^8.4.38",
    "prettier": "^3.3.2",
    "tailwindcss": "^3.4.4",
    "typescript": "~5.3.0",
    "vite": "^5.0.11",
    "vue-tsc": "^2.0.24"
